
function AccountBalance(yao, amount){
    console.log(yao, amount)
}

var accountObj={
Balance: 100,
Id:123
}

function PrintAccount(callBackFunc, accInfo, name){
    callBackFunc("The account information is:", accInfo);
}

PrintAccount(AccountBalance, accountObj);


